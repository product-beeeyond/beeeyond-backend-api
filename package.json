		"bcryptjs": "^2.4.3",
		"jsonwebtoken": "^9.0.2",
		"joi": "^17.11.0",
		"express-rate-limit": "^7.1.5",
		"sequelize": "^6.35.2",
		"pg": "^8.11.3",
		"pg-hstore": "^2.3.4",
		"stellar-sdk": "^11.2.2",
		"twilio": "^4.19.3",
		"resend": "^2.1.0",
