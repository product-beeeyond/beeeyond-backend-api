{
  "name": "brikkle-backend",
  "version": "1.0.0",
  "description": "Brikkle Real Estate Tokenization Platform Backend",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon dist/app.ts",
    "build": "tsc",
    "compile": "tsc -w",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "lint:check": "eslint 'src/**/*.{js,ts}'",
    "lint:unused": "eslint --fix-dry-run \"src/**/*.{js,ts}\"",
    "start": "node dist/app.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop"
  },
  "dependencies": {
    "@stellar/stellar-sdk": "^13.3.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "cron": "^3.5.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "rate-limit-redis": "^4.2.2",
    "redis": "^4.7.1",
    "resend": "^2.1.0",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "twilio": "^4.23.0",
    "uuid": "^9.0.1",
    "validator": "^13.15.15",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/uuid": "^9.0.8",
    "@types/validator": "^13.15.2",
    "eslint": "^9.32.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
