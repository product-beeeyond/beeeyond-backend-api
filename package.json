	"version": "1.0.0",
	"description": "Brikkle Real Estate Tokenization Platform Backend",
	"main": "dist/app.js",
	"scripts": {
		"dev": "nodemon src/app.ts",
		"build": "tsc",
		"start": "node dist/app.js",
		"migrate": "npx sequelize-cli db:migrate",
